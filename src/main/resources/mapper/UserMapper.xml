<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.longcai.dao.UserMapper">
	<select id="findAll" resultType="com.longcai.entity.User">
		SELECT * FROM tb_user
	</select>

	<insert id="insertUser" useGeneratedKeys="true" keyProperty="id" keyColumn="id" parameterType="com.longcai.entity.User">
		insert into tb_user(username,password) values (#{userName},#{password})
	</insert>

</mapper>